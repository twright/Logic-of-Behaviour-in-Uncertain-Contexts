continuous reachability
{
 state var x0,x1,x2,t

 par
 {
   k = 1
 }

 setting
 {
  fixed steps 0.0001
  time 10
  remainder estimation 1e-5
  identity precondition
  gnuplot octagon x0,x2
  fixed orders 8
  cutoff 1e-15
  precision 100
  output bug2b
  print on
 }
	
 nonpoly ode {
  x0' = (x0 + [0.0499999999999999, 0.0500000000000001]*x1*x2 + x0*x2 + [0.0499999999999999, 0.0500000000000001]*[-1.00000000000000, -1.00000000000000])/(x0 + [0.0499999999999999, 0.0500000000000001]*x2 + [0.0499999999999999, 0.0500000000000001])
  x1' = [0.000000000000000, 0.000000000000000]
  x2' = (-(x0 + [0.0499999999999999, 0.0500000000000001]*x1*x2 + x0*x2 + [0.0499999999999999, 0.0500000000000001]*[-1.00000000000000, -1.00000000000000]))/(x0 + [0.0499999999999999, 0.0500000000000001]*x2 + [0.0499999999999999, 0.0500000000000001])
  t'  = 1
 }
 
 init
 {    
   x0 in [0,0]
   x1 in [3,3]
   x2 in [0.1,0.1]
   t  in [0,0]
 }
}
