continuous reachability
{
 state var t,x,y


 setting
 {
  fixed steps 0.1
  time 5
  remainder estimation 1e-3
  identity precondition
  gnuplot octagon x,y
  fixed orders 3
  cutoff 1e-7
  precision 53
  plot output genetic
  print on
 }

 poly ode 2 # the symbolic remainder senario which can only be used with "poly ode 1" and "nonpoly ode"
 {
  t' = 1
  x' = -y
  y' = x
 }

 init
 {
  t in [0, 0]
  x in [4,5]
  y in [1,2]
 }
}
